<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version2/core" level="3" version="2">
  <model name="Dimerization">
    <listOfCompartments>
      <compartment id="vol" spatialDimensions="3" size="1" constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="dimer" compartment="vol" initialAmount="0" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="monomer" compartment="vol" initialAmount="30" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="dimer_1" compartment="vol" initialAmount="0" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="monomer_1" compartment="vol" initialAmount="30" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="k_c" value="0.005" constant="true"/>
      <parameter id="k_d" value="0.08" constant="true"/>
      <parameter id="k_c_1" value="0.005" constant="true"/>
      <parameter id="k_d_1" value="0.08" constant="true"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="r_creation" reversible="true">
        <listOfReactants>
          <speciesReference species="monomer" stoichiometry="2" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="dimer" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> k_c </ci>
                <ci> monomer </ci>
                <apply>
                  <minus/>
                  <ci> monomer </ci>
                  <cn type="integer"> 1 </cn>
                </apply>
              </apply>
              <ci> vol </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="r_dissociation" reversible="true">
        <listOfReactants>
          <speciesReference species="dimer" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="monomer" stoichiometry="2" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> k_d </ci>
              <ci> dimer </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="r_creation_1" reversible="true">
        <listOfReactants>
          <speciesReference species="monomer_1" stoichiometry="2" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="dimer_1" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> k_c_1 </ci>
                <ci> monomer_1 </ci>
                <apply>
                  <minus/>
                  <ci> monomer_1 </ci>
                  <cn type="integer"> 1 </cn>
                </apply>
              </apply>
              <ci> vol </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="r_dissociation_1" reversible="true">
        <listOfReactants>
          <speciesReference species="dimer_1" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="monomer_1" stoichiometry="2" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> k_d_1 </ci>
              <ci> dimer_1 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
